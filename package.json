    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^5.0.1",
    "enzyme": "^3.11.0",
    "loader-utils": "^2.0.0",
    "localforage": "^1.9.0",
    "lodash": "^4.17.21",
    "mobx": "^6.3.2",
