    "serializr": "^2.0.3",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "webpack serve --mode development --env development --hot --port 8080",
    "build": "webpack",
    "lint": "eslint src",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "transform": [
      "es2021"
    ]
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.ts",
    "setupFiles": [
      "jest-plugin-unhandled-promise/setup",
      "raf/polyfill"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "jsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
